{
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "google": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      1,
      "single"
    ],
    "semi": [
      1,
      "always"
    ],
    "no-unused-vars": 1
  }
}